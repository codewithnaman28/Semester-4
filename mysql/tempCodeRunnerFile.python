import tkinter as tk

root = tk.Tk()
root.title("Smartwatch GUI")
root.geometry("200x200")

# Adding a label for displaying time
time_label = tk.Label(root, text="00:00:00", font=("Helvetica", 20))
time_label.pack(pady=20)

# Adding a button for setting alarm
alarm_button = tk.Button(root, text="Set Alarm", width=10, height=2)
alarm_button.pack()

root.mainloop()
